    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "^1.1.5",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "nib": "^1.1.2",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.13",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "rupture": "^0.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
